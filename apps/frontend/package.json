{
    "name": "area-frontend",
    "version": "0.0.1",
    "description": "Frontend app for the AREA project.",
    "private": true,
    "license": "UNLICENSED",
    "type": "module",
    "scripts": {
        "dev": "npm-run-all --parallel vite:dev i18n",
        "dev:host": "npm-run-all --parallel vite:dev:host i18n",
        "vite:dev": "vite dev",
        "vite:dev:host": "vite dev --host",
        "prod": "cross-env PORT=8081 node build/index.js",
        "build": "npm run i18n:nowatch && vite build",
        "build:ci": "npm run i18n:nowatch && cross-env CYPRESS_COVERAGE=true vite build",
        "preview": "vite preview",
        "i18n": "typesafe-i18n",
        "i18n:nowatch": "typesafe-i18n --no-watch",
        "i18n:ci": "npm run i18n:nowatch",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "dependencies": {
        "bits-ui": "^0.21.16",
        "clsx": "^2.1.1",
        "cmdk-sv": "^0.0.18",
        "lucide-svelte": "^0.446.0",
        "mode-watcher": "^0.4.1",
        "qrcode": "^1.5.4",
        "tailwind-merge": "^2.5.2",
        "tailwind-variants": "^0.2.1",
        "typesafe-i18n": "^5.26.2"
    },
    "devDependencies": {
        "@sveltejs/adapter-node": "^5.2.5",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@types/eslint": "^9.6.0",
        "@types/node": "^22.6.0",
        "@types/qrcode": "^1.5.5",
        "autoprefixer": "^10.4.20",
        "cross-env": "^7.0.3",
        "cypress": "^13.15.0",
        "cypress-plugin-dotenv": "^1.3.0",
        "eslint": "^9.0.0",
        "eslint-plugin-svelte": "^2.36.0",
        "globals": "^15.0.0",
        "npm-run-all": "^4.1.5",
        "start-server-and-test": "^2.0.8",
        "svelte": "^4.2.7",
        "svelte-check": "^4.0.0",
        "tailwindcss": "^3.4.9",
        "typescript": "~5.5.0",
        "typescript-eslint": "^8.0.0",
        "vite": "^5.0.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zowks/B-DEV-500-area.git"
    },
    "bugs": {
        "url": "https://github.com/zowks/B-DEV-500-area/issues"
    },
    "homepage": "https://github.com/zowks/B-DEV-500-area#readme"
}

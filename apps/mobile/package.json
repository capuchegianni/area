{
    "name": "area-mobile",
    "main": "expo-router/entry",
    "version": "0.0.1",
    "description": "Mobile app for the AREA project.",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "npm run build:android",
        "build:android": "eas build -p android --profile preview",
        "dev": "npm run start",
        "dev:android": "npm run start:android",
        "dev:ios": "npm run start:ios",
        "dev:web": "npm run start:web",
        "start": "expo start -c",
        "start:android": "expo start -c --android",
        "start:ios": "expo start -c --ios",
        "start:web": "expo start -c --web",
        "android": "npm run start:android",
        "ios": "npm run start:ios",
        "web": "npm run start:web",
        "test": "jest",
        "test:watch": "jest --watchAll",
        "test:updatesnapshots": "jest -u --coverage=false",
        "lint": "expo lint",
        "lint:fix": "eslint . --fix",
        "clean": "rm -rf .expo node_modules",
        "postinstall": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css"
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "**/*.{ts,tsx,js,jsx}",
            "!**/coverage/**",
            "!**/node_modules/**",
            "!**/babel.config.js",
            "!**/metro.config.js",
            "!**/tailwind.config.js",
            "!**/expo-env.d.ts",
            "!**/nativewind-env.d.ts",
            "!**/.expo/**",
            "!./index.js"
        ]
    },
    "directories": {
        "lib": "lib"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "1.23.1",
        "@react-navigation/native": "^6.0.2",
        "@rn-primitives/avatar": "~1.0.4",
        "@rn-primitives/checkbox": "^1.0.4",
        "@rn-primitives/portal": "~1.0.3",
        "@rn-primitives/progress": "~1.0.3",
        "@rn-primitives/slot": "~1.0.3",
        "@rn-primitives/tooltip": "~1.0.3",
        "@rn-primitives/types": "~1.0.3",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "crypto-js": "^4.2.0",
        "expo": "~51.0.35",
        "expo-linking": "~6.3.1",
        "expo-localization": "~15.0.3",
        "expo-navigation-bar": "~3.0.7",
        "expo-router": "~3.5.20",
        "expo-splash-screen": "~0.27.5",
        "expo-status-bar": "~1.12.1",
        "expo-system-ui": "~3.0.7",
        "i18next": "^23.15.1",
        "lucide-react-native": "^0.378.0",
        "nativewind": "^4.0.33",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-i18next": "^15.0.2",
        "react-native": "0.74.5",
        "react-native-reanimated": "~3.10.1",
        "react-native-safe-area-context": "4.10.5",
        "react-native-screens": "~3.31.1",
        "react-native-svg": "15.2.0",
        "react-native-web": "~0.19.6",
        "react-use": "^17.5.1",
        "tailwind-merge": "^2.2.1",
        "tailwindcss": "3.3.5",
        "tailwindcss-animate": "^1.0.7",
        "zustand": "^4.4.7"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@testing-library/react-native": "^12.7.2",
        "@types/crypto-js": "^4.2.2",
        "@types/jest": "^29.5.13",
        "@types/react": "~18.2.79",
        "@types/react-test-renderer": "^18.3.0",
        "eslint": "^8.57.0",
        "eslint-config-expo": "^7.1.2",
        "eslint-plugin-testing-library": "^6.3.0",
        "jest": "^29.7.0",
        "jest-expo": "^51.0.4",
        "react-test-renderer": "18.2.0",
        "typescript": "~5.3.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zowks/B-DEV-500-area.git"
    },
    "bugs": {
        "url": "https://github.com/zowks/B-DEV-500-area/issues"
    },
    "homepage": "https://github.com/zowks/B-DEV-500-area#readme"
}
